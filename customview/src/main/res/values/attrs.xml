<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StateFrameLayout">

        <!--Status Messages-->
        <attr name="loading_message" format="string" />
        <attr name="error_message" format="string" />
        <attr name="net_error_message" format="string" />
        <attr name="empty_message" format="string" />


        <!--Status Drawable-->
        <attr name="loading_drawable" format="reference" />
        <attr name="error_drawable" format="reference" />
        <attr name="net_error_drawable" format="reference" />
        <attr name="empty_drawable" format="reference" />


    </declare-styleable>

    <!--折线图-->
    <declare-styleable name="LineView">
        <attr name="line_colors" format="color"/>
        <attr name="line_Width" format="dimension"/>
        <attr name="dot_Radius" format="dimension"/>
        <attr name="xTextSize" format="dimension"/>
        <attr name="yTextSize" format="dimension"/>
        <attr name="xTextColor" format="color"/>
        <attr name="yTextColor" format="color"/>
        <attr name="dot_Width" format="dimension"/>

    </declare-styleable>

</resources>